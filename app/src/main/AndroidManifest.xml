<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.college.fitness">

    <application
        android:name="com.college.fitness.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollegeTermProject">
        <activity
            android:name="com.college.fitness.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.CollegeTermProject.NoActionBar">
            <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name="com.college.fitness.RegistrationActivity"
            android:label="@string/title_activity_registration"
            android:theme="@style/Theme.CollegeTermProject" />
        <activity
            android:name="com.college.fitness.UpdateUserActivity"
            android:label="@string/title_activity_update_user"
            android:theme="@style/Theme.CollegeTermProject" />
        <activity
            android:name="com.college.fitness.AddItemActivity"
            android:label="@string/title_activity_add_item"
            android:theme="@style/Theme.CollegeTermProject" />
        <activity
            android:name="com.college.fitness.LoginActivity"
            android:theme="@style/Theme.CollegeTermProject.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>